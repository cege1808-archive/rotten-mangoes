List of Users
<%=paginate @all_users %>
<%= link_to "Add new user", new_admin_user_path %>

<% @all_users.each do |user| %>
  <p><%= link_to user.full_name , admin_user_path(user) %>(<%= link_to "edit", edit_admin_user_path(user) %>, <%= link_to "delete", admin_user_path(user), method: :delete, data: {confirm: "You sure?"} %>, <%= link_to "shadow", admin_shadow_sessions_path(user_id: user.id), method: :post %> )</p>
<%end %>
